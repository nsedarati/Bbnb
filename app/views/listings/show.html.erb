<h1>Listings#show</h1>
<p>Find me in app/views/listings/show.html.erb</p>
<div id="listing">
  <div class="listing">
    <%= image_tag @listing.avatar_url(:thumb).to_s %>
    <div class="listing_name"><%= @listing.listing_name %></div>
    <div class="actions">
      <%= link_to "edit", edit_listing_path(@listing) %> |
        <%= link_to "remove", @listing, :confirm => 'Are you sure?', :method => :delete %>
    </div>
  </div>
  <div class="clear"></div>
</div>